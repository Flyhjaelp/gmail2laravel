{
    "name": "flyhjaelp/gmail2laravel",
    "description": "A Laravel package to help manage multiple Gmail Clients on the same platform.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Traian Roman",
            "email": "dtr@flyhjaelp.dk",
            "homepage": "https://flyhjaelp.dk"
        }
    ],
    "homepage": "https://github.com/flyhjaelp/gmail2laravel",
    "keywords": ["Laravel", "gmail2laravel"],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6|^5.7",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },

    "autoload": {
        "psr-4": {
            "flyhjaelp\\Gmail2Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "flyhjaelp\\Gmail2Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "flyhjaelp\\Gmail2Laravel\\Gmail2LaravelServiceProvider"
            ]
        }
    }
}
